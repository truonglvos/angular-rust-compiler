{
  "name": "angular-rust-binding",
  "version": "0.2.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "angular-binding",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-apple-darwin",
        "aarch64-apple-darwin"
      ]
    }
  },
  "scripts": {
    "build": "napi build --platform --cargo-name angular_binding",
    "build:debug": "napi build --platform --cargo-name angular_binding",
    "build:release": "napi build --platform --cargo-name angular_binding --release"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  }
}
